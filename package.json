{
  "name": "aa_scraper",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "precommit": "npm test",
    "prepush": "npm test",
    "test": "mocha **/*.test.js --timeout 5000 --recursive --exit",
    "test:coverage": "export NODE_ENV=testing && nyc mocha test/*.js --timeout 5000 --recursive --exit",
    "scrape": "node ./app/index.js",
    "scrape:stragglers": "node ./app/stragglers.js",
    "dev": "export DEBUG=* && nodemon --ignore 'neo4j_data/**/*.*' --ignore 'logs/**/*.*' ./app/index.js",
    "dev:stragglers": "export DEBUG=* && nodemon --ignore 'neo4j_data/**/*.*' --ignore 'logs/**/*.*' ./app/stragglers.js",
    "lint": "./node_modules/eslint/bin/eslint.js app/*.js app/**/*.js test/**/*.js"
  },
  "author": "Johannes Skjærbæk",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "mocha": "^5.0.2",
    "nodemon": "^1.17.1",
    "nyc": "^11.4.1"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "cheerio": "^1.0.0-rc.2",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "neo4j-driver": "^1.5.3",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "title-case": "^2.1.1"
  }
}
